{
  "expo": {
    "name": "masonry-react-native-expo",
    "slug": "masonry-react-native-expo",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.masonry-react-native-expo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.masonryreactnativeexpo"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "build": {
        "serviceworker": {
          "register": true,
          "scope": "/",
          "updateViaCache": "none"
        }
      }
    }
  }
}
